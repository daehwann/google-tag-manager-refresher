{
  "name": "Google Tag Manager Refresher",
  "description": "Refresh current workspace",
  "icons": { "16": "icon16.png",
    "48": "icon32.png",
   "128": "icon128.png" 
  },
  "version": "4.1",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Refresh GTM Workspace"
  },
  "manifest_version": 2
}
